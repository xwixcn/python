		<script type="text/javascript" src="/static/jquery.easypage.js">
		</script>
		<script type="text/javascript">
			//contentclass   要分页内容的class名称 默认的为contentlist
			//navigateid		 放置导航按钮的位置id 默认为mynavigate
			//everycount 		 每页显示多少个
			//navigatecount	 导航按钮开始显示多少个，从第二个开始显示为双倍	
			$(
				function(){
					$.easypage({'contentclass':'typedata','navigateid':'navigatediv','everycount':5,'navigatecount':5});	
				}
			)
		</script>	

