$def with(num,data)
<html>
    <head>
        <title>roundIDdata Page</title>
        <meta http-equiv="content-type" content="text/html; charset=utf-8">
          <script type="text/javascript">
                       var nodetr=document.getElementByTagName('td');
                       
                       </script>

    </head>
    <body>
        <div id="headerbar">
           <ul id="accountnav">
                <li><a href="/home">回到首页</a> </li>
           </ul>
        </div>

        <table align="center" border="1" cellspacing="0" bordercolor="#000000" width="100%">
            <caption class="STYLE10" >
                <font face="verdana" size="8" color="#A52A2A">第$num轮详细信息</font>
            </caption>
            <tr height="30px" bgcolor="#3399CC" >
                <td align='center'>patternstr</td><td>
                <table align="center" border="0" cellspacing="0" width="100%" >
                    <td  align="center" valign='top'width="20%">问句</td>
                    <td  align="center" valign='top' width="10%" >本次状态</td>
                     <td  align="center" valign='top' width="10%" >上次状态</td>
                    <td  align="center" valign='top'width="35%">详细信息</td>
                    <td  align="center" valign='top'width="35%" colspan="3">查看问句结果</td>

                </table>
            </tr>
            $for result in data.iterkeys():
                <tr >
                <td width="25%" align="center" ><a>$result</a></td>
                <td>
                <table id ="table_$result" align="center" border="1"   width="100%" >
                    $for dic in data[result]:
                            <tr id="resonID">
                            <td  align="center" valign='top'width="20%">$dic["name"]</td>
                            <td  align="center" valign='top' width="10%" >$dic["status"]</td>
                            <td  align="center" valign='top' width="10%" id="td_$result">$dic["last_status"]</td>
                            <td  align="center" valign='top'width="35%">$dic["detailInfo"]</td>
                            <td ><a href="http://x.10jqka.com.cn/stockpick/search?tid=stockpick&w=$dic['name']" target="_blank">线上</a></td>
                            <td ><a href="http://s.10jqka.com.cn/stockpick/search?tid=stockpick&w=$dic['name']" target="_blank">searchtest</a></td>
                            <td ><a href="http://192.168.23.105/stockpick/search?tid=stockpick&w=$dic['name']" target="_blank">105</a></td>
                             </tr>

                    </table></td>


        </table>
         <script type="text/javascript">
         var nodetr=document.getElementsByTagName('td');
         document.write(nodetr[0].textContent);
         var i=0;
         for(i=0;i<nodetr.length;i++)
         {
         var text=nodetr[i].textContent;
         if(text=="P")
         {nodetr[i].parentNode.style.cssText="background:#FF0000";
         }
         
                     }  
        </script>

    </body>
</html>
