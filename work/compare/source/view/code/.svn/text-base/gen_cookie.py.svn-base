# -*- coding: utf-8 -*-

import cookielib
import urllib2

def gen_cookie( ):
    cj = cookielib.LWPCookieJar( "cookie.txt" )
    cjhandler = urllib2.HTTPCookieProcessor( cj )
    opener = urllib2.build_opener( cjhandler )
    urllib2.install_opener( opener )
    resp = urllib2.urlopen( 'http://x.10jqka.com.cn/stockpick/search?preParams=&ts=1&f=1&qs=1&tid=stockpick&w=2012%E4%BA%8C%E5%AD%A3%E5%BA%A6%E6%AF%94%E5%8E%BB%E5%B9%B4%E5%87%80%E5%88%A9%E6%B6%A6%E5%A2%9E%E9%95%BF%E6%9C%80%E9%AB%98%E7%9A%84%E8%82%A1%E7%A5%A8+&queryarea=all&source=data' )
    cj.save()
    resp.read()


