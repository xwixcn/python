<html xmlns:math="http://exslt.org/math" xmlns:testng="http://testng.org">
    <head>
        <title>Compare Page</title>
        <meta http-equiv="content-type" content="text/html; charset=utf-8">
        <meta http-equiv="pragma" content="no-cache">
        <meta http-equiv="cache-control" content="max-age=0">
        <meta http-equiv="cache-control" content="no-cache">
        <meta http-equiv="cache-control" content="no-store">
    </head>
    <body>
        <div id="navigation">
            <table width="10%" cellpadding="0" border="1" cellspacing="0">
                <tr align="right">
                    <th>回到首页</th>
                </tr>
            </table>

        </div>

        <table width="100%" style="font-size: 16px; margin-bottom: 10px;" cellspacing="1">
            <tr>
                <td width="100%"> Test case <b>Test</b></td>
                <td style="background-color: #FFBBBB; padding: 3px 3px 3px 0;" align="center" >
                <div style="width: 80px;" >
                    基准环境
                </div></td>
                <td style="background-color: #FFBBBB; padding: 3px 3px 3px 0;" align="center" >
                <div style="width: 80px;" >
                    测试环境
                </div></td>
                <td style="background-color: #FFBBBB; padding: 3px 3px 3px 0;" align="center">
                <div style="width: 80px;">
                    case总数
                </div></td>
                <td style="background-color: lightgreen; padding: 3px 3px 3px 0;" align="center">
                <div style="width: 80px;">
                    成功数目
                </div></td>
                <td style="background-color: #FFFFBB; padding: 3px 3px 3px 0;" align="center">
                <div style="width: 80px;">
                    失败数目
                </div></td>
            </tr>
            <tr>
                <td width="100%"><b></b></td>
                $for i in [baseurl,testurl]:
                    <td style="background-color: #FFBBBB; padding: 3px 3px 3px 0;" align="center">
                    $if i.startswith("http://s"):
                        <div  style="width: 80px;" >searchtest</div></td>
                    $elif i.startswith("http://x"):
                        <div  style="width: 80px;" >正式环境</div></td>
                    $else:
                        <div  style="width: 80px;" >192.168.23.105</div></td>
                <td style="background-color: #FFBBBB; padding: 3px 3px 3px 0;" align="center">
                <div style="width: 50px;">
                    $data["total"]
                </div></td>
                <td style="background-color: lightgreen; padding: 3px 3px 3px 0;" align="center">
                <div style="width: 50px;">
                    $data["pass"]
                </div></td>
                <td style="background-color: #FFFFBB; padding: 3px 3px 3px 0;" align="center">
                <div style="width: 50px;">
                    $data["fail"]
                </div></td>
            </tr>
        </table>
        <div id="testMethodsByStatus">
            <table  cellpadding="0" width="100%" align="center" border="1" cellspacing="0" bordercolor="#000000" width="1000">
                <tr height="30px" bgcolor="#3399CC" >

                    <th align='center'>问句名称</th>
                    <th width="50%" align="center" >问句失败原因</th>
                    <th width="20%" align="center" colspan="2" >问句比对结果</th>
                    <th width="25%" align="center" colspan="3" > 查看问句</th>

                </tr>
                $for elem in data["result"]:
                <tr >
                    <td  align="center" valign='top'>$elem["sentence"]</td>
                    <td  align="center" valign='top'>数据不一致</td>
                    <td  align="center" ><a href="$baseurl/stockpick/search?tid=stockpick&w=$elem['sentence']" target="_blank">基准结果数目:$elem["bench_num"]</a></td>
                    <td  align="center" ><a href="$testurl/stockpick/search?tid=stockpick&w=$elem['sentence']" target="_blank">测试结果数目:$elem["test_num"]</a></td>
                    <td ><a href="http://x.10jqka.com.cn/stockpick/search?tid=stockpick&w=$elem['sentence']" target="blank">线上</a></td>
                    <td ><a href="http://s.10jqka.com.cn/stockpick/search?tid=stockpick&w=$elem['sentence']" target="blank">searchtest</a></td>
                    <td ><a href="http://192.168.23.105/stockpick/search?tid=stockpick&w=$elem['sentence']" target="blank">105</a></td>

                </tr>
            </table>
        </div>
    </body>
</html>