$def with(data)
<html>
<head>
<title>
test Page!
</title>
<script type="text/javascript">
function change_readonly()
{
    var nodelist=document.getElementsByName("lname");
    var i=0;
    
    for(i=0;i<nodelist.length;i++)
    {
     nodelist[i].readOnly=false;
    }

} </script>
</head>
<body>
    <div id="txt"></div>
    <div>
<form action="/config/edit" method="post" >
    $for section,content in data.iteritems():   
        <p>模块:$section</p>
        $for elem in content:
			属性名: <input type="text" name="fname" value=$elem[0] size=15 readonly=true />
			属性值: <input type="text" name="lname" value=$elem[1] size=38  readonly=true />
		<p></p>
<input type="button" onclick="change_readonly()" value="修改" />
<input type="submit" value="提交" />
<input type="reset" value="重置" />
</form>

</div>
</body>
</html>
