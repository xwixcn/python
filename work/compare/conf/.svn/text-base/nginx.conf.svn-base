
#user  nobody;
worker_processes  1;

#error_log  logs/error.log;
#error_log  logs/error.log  notice;
#error_log  logs/error.log  info;

#pid        logs/nginx.pid;


events {
    worker_connections  10220;
}


http {
    include       mime.types;
    default_type  application/octet-stream;

    #log_format  main  '$remote_addr - $remote_user [$time_local] "$request" '
    #                  '$status $body_bytes_sent "$http_referer" '
    #                  '"$http_user_agent" "$http_x_forwarded_for"';

    #access_log  logs/access.log  main;

    sendfile        on;
    #tcp_nopush     on;

    #keepalive_timeout  0;
    keepalive_timeout  65;

    #gzip  on;

    server {
        listen       9001;
        server_name  192.168.23.75;

        location / {
        fastcgi_connect_timeout 60;
        fastcgi_send_timeout 180;
        fastcgi_read_timeout 180;
        fastcgi_buffer_size 128k;
        fastcgi_buffers 4 256k;
        fastcgi_busy_buffers_size 256k;
        fastcgi_temp_file_write_size 256k;
        fastcgi_intercept_errors on;
        include uwsgi_params;
        uwsgi_pass 127.0.0.1:9090;
        }

        access_log /home/hudson/hbl/Webmonitor/monitorWeb/log/nginx_access.log;
        error_log /home/hudson/hbl/Webmonitor/monitorWeb/log/nginx_error.log ;

        error_page  404              /404.html;

        error_page   500 502 503 504  /50x.html;
        location = /50x.html {
            root   html;
        }
    }


    
    server {
        listen       9002;
        server_name  192.168.23.75;

        location / {
            include uwsgi_params;
            uwsgi_pass 127.0.0.1:9091;
        }
        access_log /home/hudson/hbl/Webmonitor/compare/log/nginx_access.log;
        error_log /home/hudson/hbl/Webmonitor/compare/log/nginx_access.log;
        error_page  404              /404.html;
        error_page   500 502 503 504  /50x.html;

    }


    # HTTPS server
